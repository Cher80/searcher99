<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  
  <!-- Servlets -->
  <servlet>
    <servlet-name>RPCSControllerImpl</servlet-name>
    <servlet-class>com.searcher9.server.RPCSControllerImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RPCSControllerImpl</servlet-name>
    <url-pattern>searcher9/communicator</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Searcher9.html</welcome-file>
  </welcome-file-list>

</web-app>
